{
  "name": "@adobe/helix-project-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "Starter project for Adobe Helix",
  "browser": {
    "@magento/venia-drivers": "src/drivers"
  },
  "scripts": {
    "pwa:buildpack": "buildpack",
    "pwa:build": "npm run pwa:build:prod",
    "pwa:build:dev": "npm run clean && webpack --no-progress --env.mode development",
    "pwa:build:prod": "npm run clean && webpack --no-progress --env.mode production",
    "clean": "rimraf ../scripts/pwa-dist",
    "test": "wtr \"./test/**/*.test.js\" --node-resolve --port=2000 --coverage",
    "test:watch": "npm test -- --watch",
    "lint:js": "eslint .",
    "lint:css": "stylelint 'blocks/**/*.css' 'styles/*.css'",
    "lint": "npm run lint:js && npm run lint:css",
    "start": "hlx up --print-index",
    "import": "hlx import --cache .cache/",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adobe/helix-project-boilerplate.git"
  },
